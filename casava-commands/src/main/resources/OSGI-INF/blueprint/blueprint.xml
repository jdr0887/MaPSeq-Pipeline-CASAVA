<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" default-activation="lazy">

  <command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">
    <command name="mapseq/save-demultiplexed-stats-attributes">
      <action class="edu.unc.mapseq.commands.casava.SaveDemultiplexedStatsAttributesAction">
        <property name="maPSeqDAOBean" ref="maPSeqDAOBean" />
      </action>
    </command>
    <command name="mapseq/run-casava-pipeline">
      <action class="edu.unc.mapseq.commands.casava.RunPipelineAction">
        <property name="maPSeqDAOBean" ref="maPSeqDAOBean" />
        <property name="maPSeqConfigurationService" ref="maPSeqConfigurationService" />
      </action>
    </command>
    <command name="mapseq/save-observed-cluster-density-attributes">
      <action class="edu.unc.mapseq.commands.casava.SaveObservedClusterDensityAttributesAction">
        <property name="maPSeqDAOBean" ref="maPSeqDAOBean" />
        <property name="maPSeqConfigurationService" ref="maPSeqConfigurationService" />
      </action>
    </command>
  </command-bundle>

  <reference id="maPSeqConfigurationService" interface="edu.unc.mapseq.config.MaPSeqConfigurationService" />
  <reference id="accountDAO" interface="edu.unc.mapseq.dao.AccountDAO" />
  <reference id="accountGroupDAO" interface="edu.unc.mapseq.dao.AccountGroupDAO" />
  <reference id="fileDataDAO" interface="edu.unc.mapseq.dao.FileDataDAO" />
  <reference id="HTSFSampleDAO" interface="edu.unc.mapseq.dao.HTSFSampleDAO" />
  <reference id="platformDAO" interface="edu.unc.mapseq.dao.PlatformDAO" />
  <reference id="jobDAO" interface="edu.unc.mapseq.dao.JobDAO" />
  <reference id="studyDAO" interface="edu.unc.mapseq.dao.StudyDAO" />
  <reference id="sequencerRunDAO" interface="edu.unc.mapseq.dao.SequencerRunDAO" />
  <reference id="workflowDAO" interface="edu.unc.mapseq.dao.WorkflowDAO" />
  <reference id="workflowPlanDAO" interface="edu.unc.mapseq.dao.WorkflowPlanDAO" />
  <reference id="workflowRunDAO" interface="edu.unc.mapseq.dao.WorkflowRunDAO" />

  <bean id="maPSeqDAOBean" class="edu.unc.mapseq.dao.MaPSeqDAOBean">
    <property name="accountDAO" ref="accountDAO" />
    <property name="accountGroupDAO" ref="accountGroupDAO" />
    <property name="fileDataDAO" ref="fileDataDAO" />
    <property name="HTSFSampleDAO" ref="HTSFSampleDAO" />
    <property name="platformDAO" ref="platformDAO" />
    <property name="jobDAO" ref="jobDAO" />
    <property name="studyDAO" ref="studyDAO" />
    <property name="sequencerRunDAO" ref="sequencerRunDAO" />
    <property name="workflowDAO" ref="workflowDAO" />
    <property name="workflowPlanDAO" ref="workflowPlanDAO" />
    <property name="workflowRunDAO" ref="workflowRunDAO" />
  </bean>

</blueprint>
